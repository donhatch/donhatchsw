                            {
                                double At1[] = VecMath.lerp(model.A0,model.A1,model.t1);
                                double frac = VecMath.dot(VecMath.vmv(thisPoint,model.A0),VecMath.vmv(At1,model.A0)) / VecMath.normsqrd(VecMath.vmv(At1,model.A0));
