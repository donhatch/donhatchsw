        public static void HSVtoRGB(float h, float s, float v, float rgb[/*3*/])
        {
            while (h < 0)
                h++;
            while (h >= 1)
                h--;

            int segment = (int)(h*6);

            float frac = (6*h)-segment;
            if (segment % 2 == 1)
                frac = 1 - frac;

            float major = v;
            float minor = (1-s)*v;
            float middle = frac * major + (1-frac) * minor;

            rgb[(segment+1)/2 % 3] = major;
            rgb[(segment+4)/2 % 3] = minor;
            rgb[(7-segment) % 3] = middle;
        } // HSVtoRGB


